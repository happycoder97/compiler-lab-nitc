type Struct {
    int a;
    int b;
}

class Foo {
    int a, b;

    int init(int a, int b) {
        self.a = a;
        self.b = b;
    }

    int set(Struct s) {
        s = alloc();
        s.a = self.a;
        s.b = self.b;
    }

    int unify(Struct s1, Struct s2) {
        s1 = s2;
    }

}

Foo f1;
Struct s1, s2;

int main() {
    initialize();
    f1 = new(Foo);
    f1.init(10, 20);
    f1.set(s1);
    write(s1.a);
    write(s1.b);

    f1.unify(s2, s1);
    s1.a = 30;
    s1.b = 40;
    write(s2.a);
    write(s2.b);
    s2.a = 100;
    s2.b = 200;
    write(s1.a);
    write(s1.b);
}
